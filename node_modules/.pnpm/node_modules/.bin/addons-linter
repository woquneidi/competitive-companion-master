#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules/addons-linter/bin/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules/addons-linter/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules/addons-linter/bin/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules/addons-linter/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/addons-linter@7.13.0/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../addons-linter/bin/addons-linter" "$@"
else
  exec node  "$basedir/../addons-linter/bin/addons-linter" "$@"
fi
