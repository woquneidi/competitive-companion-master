#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/bin/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/bin/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/@devicefarmer+adbkit@3.3.8/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/bin/adbkit" "$@"
else
  exec node  "$basedir/../../../../../@devicefarmer+adbkit@3.3.8/node_modules/@devicefarmer/adbkit/bin/adbkit" "$@"
fi
