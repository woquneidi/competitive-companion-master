#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules/firefox-profile/lib/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules/firefox-profile/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules/firefox-profile/lib/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules/firefox-profile/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/firefox-profile@4.7.0/node_modules:/mnt/d/wqnd/Desktop/新建文件夹/competitive-companion-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../firefox-profile@4.7.0/node_modules/firefox-profile/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../firefox-profile@4.7.0/node_modules/firefox-profile/lib/cli.js" "$@"
fi
